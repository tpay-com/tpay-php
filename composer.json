{
    "name": "tpay-com/tpay-php",
    "description": "Tpay.com library",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "tpay.com",
            "homepage": "https://tpay.com"
        }
    ],
    "support": {
        "issues": "https://github.com/tpay-com/tpay-php/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "phpseclib/phpseclib": "^2 || ^3"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.3.2",
        "phpunit/phpunit": "^5.7.27 || ^9.6.10"
    },
    "autoload": {
        "psr-4": {
            "Tpay\\OriginApi\\": "src/"
        },
        "files": [
            "src/legacy_classes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tpay\\OriginApi\\Tests\\": "tests/"
        }
    },
    "extra": [
        {
            "engine": "PHP SDK"
        }
    ]
}
